<!doctype html>
<html>
  <head>
    <meta charset='utf-8' />
    <%= csrf_meta_tags %>
    <%= wicked_pdf_stylesheet_link_tag "sem1-report-card" %>
    <%= javascript_include_tag wicked_pdf_asset_base64("pdf") %>
    
  </head>
  <body onload='number_pages'>
    <div id="header">
     <%= image_tag wicked_pdf_asset_base64('ris-header.svg'), class: 'reportcard-header-image' %>
     
    </div>
    <div id="content">
      <%= yield %>
    </div>

  </body>
</html>