<h2>All Students</h2>
<%= content_tag :div, class: "student_list", id: "studentlist", data: {students: @students} do %>
<%end%>

<%= form_tag students_path, :method => 'get' do %>
Search: <%= text_field_tag 'student[search]' %>
<%= submit_tag "Search", :name => nil %>
<!-- <button class="new_student"><%#= link_to "Create Student", new_student_path %></button> -->
<% end %>



<ol>
	<% @students.each do |student| %>
	<li><%= link_to student.lastfirst, student_path(student)%></li>
	<% end %>
</ol>

