<h1>All Admin</h1>

<ul>
	<% @users.each do |user| %>
		<li><%=user.username%> - <%=link_to user.email, "mailto:#{user.email}", target: "_blank" %></li>
	<% end %>
</ul>


<h1>All Teachers</h1>

<ul>
	<% @teachers.each do |teacher| %>
		<li><%=teacher.lastfirst%> - <%=link_to teacher.email, "mailto:#{teacher.email}", target: "_blank" %></li>
	<% end %>
</ul>
