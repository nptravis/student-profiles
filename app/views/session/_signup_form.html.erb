

<h1>Sign Up</h1>

<%= form_for @user do |f| %>
	<p>
	<%= f.label "Username" %>
	<%= f.text_field :username %>
	</p>

	<p>
	<%= f.label "Password" %>
	<%= f.password_field :password %>
	</p>
	
	<p>
	<%= f.label "Password Confirmation" %>
	<%= f.password_field :password_confirmation %>
	</p>
	

	<%= f.submit "Sign Up"%>
<% end %>
